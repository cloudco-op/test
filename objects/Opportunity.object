        <url>{!REQUIRESCRIPT(&quot;/apex/DocuSign_JavaScript&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)}

// TODO: Move common methods to apex class
/**
 * Returns an initialized config object
 */
function getConfig()
{
    var config = {
        users: [],
        addUser: function (user, Role, RoleType)
        {
            user.clone = clone;
            user = user.clone();
            user.Role = Role;
            user.RoleType = RoleType;
            this.users.push(user);
        },
        getUrl: function ()
        {
            var SourceID = &apos;{!Opportunity.Id}&apos;;
            if(&apos;&apos; === SourceID)
            {
                throw &apos;This button can only be used within the context of an Opportunity&apos;;
            }

            var DST  = &apos;C52DA913-1108-4A79-A88F-2E36CC3E9B44&apos;; // DocuSign Template ID (default no template) Ex: &apos;67870A79-A0B5-4596-8AC1-CC7CC1EA01EB&apos;
            var CRL  = &apos;&apos;;  // Custom Recipient List Ex: &apos;Email~;FirstName~;LastName~;Role~SignInPersonName~;RoutingOrder~;AccessCode~;RecipientNote~;SignNow~, LoadDefaultContacts~1&apos;
            var CCTM = &apos;&apos;; // Custom Contact Type Map (default Signer) Ex: &apos;Decision Maker~Signer;Economic Buyer~CC&apos;
            var CCRM = &apos;&apos;; // Custom Contact Role Map (default config role) Ex: &apos;Decision Maker~Signer1;Economic Buyer~Carbon Copy&apos;

            if(&apos;&apos; === DST)
            {
                throw &apos;This button requires a templateID to be set&apos;;
            }
            for (var i = 0; i &lt; this.users.length; i++)
            {
                var user = this.users[i];

                CRL += user.Email ? &apos;Email~&apos; + user.Email + &apos;;&apos; : &apos;&apos;;
                CRL += user.FirstName ? &apos;FirstName~&apos; + user.FirstName + &apos;;&apos; : &apos;&apos;;
                CRL += user.LastName ? &apos;LastName~&apos; + user.LastName + &apos;;&apos; : &apos;&apos;;
                CRL += user.Role ? &apos;Role~&apos; + user.Role + &apos;;,&apos; : &apos;,&apos;;

                CCTM += user.Role + &apos;~&apos; + user.RoleType + &apos;;&apos;;

                CCRM += user.Role ? user.Role + &apos;~&apos; + user.Role + &apos;;&apos; : &apos;&apos;;
            }

            return &apos;/apex/dsfs__DocuSign_CreateEnvelope?DSEID=0&amp;SourceID=&apos; + SourceID + &apos;&amp;DST=&apos; + DST + &apos;&amp;CRL=&apos; + CRL + &apos;&amp;CCTM=&apos; + CCTM + &apos;&amp;CCRM=&apos; + CCRM + &apos;&amp;CEM=&apos; + CEM + &apos;&amp;CES=&apos; + CES;
        }
    }
    return config;
}

/**
 *
 */
var clone = function()
{
    return {
        Email: this.Email,
        FirstName: this.FirstName,
        LastName: this.LastName,
        clone: clone
    };
};

function updateOpportunitySigner(signer)
{
    sforce.apex.execute(&apos;NoesisDocusignIntegration&apos;, &apos;updateOpportunitySigner&apos;, {
        opportunityId: &apos;{!Opportunity.Id}&apos;,
        FirstName: signer.FirstName || &apos;&apos;,
        LastName: signer.LastName || &apos;&apos;,
        Title: signer.Title || &apos;&apos;,
        Email: signer.Email || &apos;&apos;,
        Phone: signer.Phone || &apos;&apos;
    });
}

/**
 * Selects the first user it finds who is in the role. This is really designed for when a single user has a role in the organization
 */
function getUserInNoesisRole(role)
{
    var users = sforce.apex.execute(&apos;NoesisDocusignIntegration&apos;, &apos;getUserInNoesisRole&apos;, {
        role: role
    });
    if (0 === users.length)
    {
        return null;
    }
    if (1 &lt; users.length)
    {
        throw &apos;More than one Noesis user found for Noesis role &quot;&apos; + role + &apos;&quot;. Please contact Tracy.&apos;;
    }
    var user = users[0];
    user.Email = user.Email || &apos;&apos;;
    if(&apos;&apos; === user.Email)
    {
        alert(&apos;The user &quot;&apos; + user.FirstName + &apos; &apos; + user.LastName + &apos;&quot; in the role &quot;&apos; + role + &apos;&quot; must have a valid email set.&apos;);
        return false;
    }
    return user;
}

function getContactInOpportunityRole(role)
{
    var contacts = sforce.apex.execute(&apos;NoesisDocusignIntegration&apos;, &apos;getContactsInOpportunityRole&apos;, {
        opportunityId: &apos;{!Opportunity.Id}&apos;,
        role: role
    });
    if(0 == contacts.length)
    {
        return null;
    }
    else if(1 &lt; contacts.length)
    {
        throw &apos;More than one contact found in the &quot;&apos; + role + &apos;&quot; role for the opportunity&apos;;
    }
    return contacts[0];
}

function validateAgreementSigner(signer)
{
    signer.FirstName = signer.FirstName || &apos;&apos;;
    if(&apos;&apos; === signer.FirstName)
    {
        throw &apos;The signer must have a valid FirstName set.&apos;;
    }

    signer.LastName = signer.LastName || &apos;&apos;;
    if(&apos;&apos; === signer.LastName)
    {
        throw &apos;The signer must have a valid LastName set.&apos;;
    }

    signer.Email = signer.Email || &apos;&apos;;
    if(&apos;&apos; === signer.Email)
    {
        throw &apos;The signer must have a valid Email set.&apos;;
    }

    signer.Phone = signer.Phone || &apos;&apos;;
    if(&apos;&apos; === signer.Phone)
    {
        throw &apos;The signer must have a valid Phone set.&apos;;
    }
}

var CES = &apos;{!Opportunity.Account} - Noesis Financing Services Agreement&apos;;

var CEM = &apos;Hello,\\n\
\\n\
Please find enclosed your Noesis Financing Services Agreement and Statement of Work.\\n\
\\n\
Please sign both documents before the agreement expiration date of {!Opportunity.Agreement_Expiration_Date__c}.\\n\
\\n\
\\n\
We look forward to helping you with your Project Financing needs.\\n\
\\n\
Thanks,\\n\
\\n\
{!Opportunity.OwnerFirstName} {!Opportunity.OwnerLastName}&apos;;

function invoke(agreementType)
{
    // Validate the opportunity
    var result = sforce.apex.execute(&apos;NoesisDocusignIntegration&apos;, &apos;validateOpportunity&apos;, {
        opportunityId: &apos;{!Opportunity.Id}&apos;,
        agreementType: agreementType
    });
    if (result[0])
    {
        alert(result[0]);
        return;
    }

    // Get all the parties
    var errors = [];
    var approver1 = getUserInNoesisRole(&apos;Approver 1&apos;);
    if(!approver1)
    {
        errors.push(&apos;* No Noesis users found for Noesis role &quot;Approver 1&quot;. Please contact Tracy.&apos;);
    }

    //var approver2 = getUserInNoesisRole(&apos;Approver 2&apos;);
    //if(!approver2)
    //{
    //    errors.push(&apos;* No Noesis users found for Noesis role &quot;Approver 2&quot;. Please contact Tracy.&apos;);
    //}

    var agreementSigner = getContactInOpportunityRole(&apos;Agreement Signer&apos;);
    if(!agreementSigner)
    {
        errors.push(&apos;* No contacts found in the &quot;Agreement Signer&quot; role for the opportunity&apos;);
    }

    if(0 &lt; errors.length)
    {
        alert(errors.join(&apos;\n&apos;));
        return;
    }

    // Override values for debugging
    var debug = false;
    if (debug)
    {
        // Override the relevant parties in debug
        approver1 = {
            Email: &apos;david.valys@gmail.com&apos;,
            FirstName: &apos;David&apos;,
            LastName: &apos;Valys-A1&apos;
        };
        //approver2 = {
        //    Email: &apos;dvalys@hotmail.com&apos;,
        //    FirstName: &apos;David&apos;,
        //    LastName: &apos;Valys-A2&apos;
        //};
        agreementSigner = {
            Email: &apos;dvalys@noesis.com&apos;,
            FirstName: &apos;David&apos;,
            LastName: &apos;Valys-AS&apos;,
            Title: &apos;Grand Poo Bah&apos;,
            Phone: &apos;555-555-5555&apos;
        };
    }

    // Validate the signer&apos;s details
    validateAgreementSigner(agreementSigner);

    // Update the opportunity with signer details so they can be bound to the doc by docusign
    updateOpportunitySigner(agreementSigner);

    // Build the envelope
    var config = getConfig();
    config.addUser(approver1, &apos;Approver 1&apos;, &apos;Signer&apos;);
    //config.addUser(approver2, &apos;Approver 2&apos;, &apos;Signer&apos;);
    //config.addUser(approver1, &apos;Approver 1 CC&apos;, &apos;CC&apos;);
    config.addUser(agreementSigner, &apos;Agreement Signer&apos;, &apos;Signer&apos;);

    // Go to preview
    window.location.href = config.getUrl();
}
invoke(&apos;NFS&apos;);</url>
